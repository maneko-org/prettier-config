{
  "name": "@maneko/prettier",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.18.3",
  "description": "Shared Prettier config for our projects",
  "license": "MIT",
  "homepage": "https://github.com/maneko-org/prettier",
  "repository": {
    "url": "https://github.com/maneko-org/prettier"
  },
  "bugs": {
    "url": "https://github.com/maneko-org/prettier/issues"
  },
  "exports": {
    ".": {
      "import": "./index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"*.js\"",
    "prepare": "husky"
  },
  "peerDependencies": {
    "prettier-plugin-tailwindcss": "^0.7.1"
  },
  "peerDependenciesMeta": {
    "prettier-plugin-tailwindcss": {
      "optional": true
    }
  },
  "dependencies": {
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "bumpp": "^10.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier-plugin-tailwindcss": "^0.7.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write"
    ]
  }
}
